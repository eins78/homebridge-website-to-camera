{
  "cameras": [
    {
      "name": "Example",

      "chromiumPath": "/Applications/Chromium.app/Contents/MacOS/Chromium",

      "url": "http://example.com",

      "height": 1200,
      "width": 1200,
      "clip": {
        "x": 70,
        "y": 100,
        "height": 320,
        "width": 480
      },

      "waitUntil": "domcontentloaded",
      "timeout": "3000",
      "runScriptOnPage": "document.body.style.zoom = 2; document.body.style.fontFamily = 'monospace'"
    }
  ]
}
